{
    "name": "jazzi",
    "version": "3.0.5",
    "main": "./dist/index.js",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Jkierem/jazzi"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "yarn build:ts && yarn build:deno",
        "build:ts": "tsc",
        "build:deno": "deno run --unstable --allow-read --allow-write scripts/build.js",
        "clean": "rm -rf ./dist && rm -rf ./deno && rm -rf ./coverage",
        "test": "rm -rf ./coverage && jest --coverage --verbose",
        "test:silent": "rm -rf ./coverage && jest --silent",
        "test:watch": "jest --verbose --watch",
        "prepublish": "yarn clean && yarn build"
    },
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "jest": "^27.1.1",
        "ts-jest": "^27.0.5",
        "typescript": "^4.5.5"
    }
}
